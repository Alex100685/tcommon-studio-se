<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="Talend"
            id="logImageCache"
            name="Log image cache"/>
      <command
            categoryId="Talend"
            id="refreshTemplates"
            name="Refresh templates"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="logImageCache"
            contextId="talend.global"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+F2"/>
      <key
            commandId="refreshTemplates"
            contextId="talend.global"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+F3"/>
   </extension>
   <extension
         point="org.talend.rcp.global_actions">
      <GlobalAction class="org.talend.core.model.action.LogImageCacheAction"/>
      <GlobalAction class="org.talend.designer.codegen.RefreshTemplatesAction"/>
   </extension>
</plugin>
