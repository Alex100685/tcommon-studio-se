<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="handler" name="Import Export Handler" schema="schema/handler.exsd"/>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.talend.repository.items.importexport.actionSet"
            label="ImportExport">
         <action
               class="org.talend.repository.items.importexport.ui.actions.ImportItemsAction"
               icon="resources/icons/import.gif"
               id="org.talend.repository.items.importexport.actions.ImportItems"
               label="%ImportItemsAction_title"
               style="push"
               toolbarPath="Default"
               tooltip="%ImportItemsAction_title">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.talend.rcp.perspective">
         <actionSet
               id="org.talend.repository.items.importexport.actionSet">
         </actionSet>
      </perspectiveExtension>
   </extension>

</plugin>
