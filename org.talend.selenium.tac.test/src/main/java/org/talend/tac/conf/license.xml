<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="false">

	<parameter name="db.userName" value="tisadmin" />
	<parameter name="db.userPassWd" value="tisadmin" />
	<parameter name="db.driver" value="org.h2.Driver" />
	<parameter name="db.url.wrong" value="wrongURL" />
	<parameter name="db.userName.wrong" value="tisadmin_wrong" />
	<parameter name="db.userPassWd.wrong" value="tisadmin_wrong" />
	<parameter name="db.driver.wrong" value="org.h2.Driver_wrong" />

	<test name="TestDbConfig">
		<classes>
			<class name="com.talend.tac.cases.dbConfig.TestDbConfig" />
			<class name="com.talend.tac.cases.dbConfig.TestDbConfigImportParameters" />
			<class name="com.talend.tac.cases.dbConfig.TestTokenINDbConfigPage"/>
		</classes>
	</test>
	<test name="TestDbConfigReload" preserve-order="true">
		<classes>
		<parameter name="db.url" value="wrongURL" />
		<parameter name="db.userName" value="wrong userName" />
		<parameter name="db.driver" value="wrong driver" />
			<class name="com.talend.tac.cases.dbConfig.TestDbConfigReload" />
		</classes>
	</test>

	<test name="TestWrongDbConfig">
		<classes>
			<class name="com.talend.tac.cases.dbConfig.TestWrongDbConfig">
				<parameter name="db.url" value="wrongURL" />
				<methods>
					<include name="testWrongURLDbConfig" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="TestWrongDbConfig">
		<classes>
			<class name="com.talend.tac.cases.dbConfig.TestWrongDbConfig">
				<parameter name="db.driver" value="wrong driver" />
				<methods>
					<include name="testWrongDriverDbConfig" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="TestWrongDbConfig">
		<classes>
			<class name="com.talend.tac.cases.dbConfig.TestWrongDbConfig">
				<parameter name="db.userName" value="wrong userName" />
				<parameter name="db.userPassWd" value="wrong Userpasswd" />
				<methods>
					<include name="testWrongAccountDbConfig" />
				</methods>
			</class>
		</classes>
	</test>
	
	<!-- 
	<test name="testLicenseAccountsCount">
		<classes>
			<class name="com.talend.tac.cases.license.TestLicenseAccountsCount"/>
		</classes>
	</test>
	 -->
</suite>

