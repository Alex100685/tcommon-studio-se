-- CREATE STAGGING TABLE
CREATE MULTISET TABLE talend.cust_teradata_stagging ,NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT
     (
      ID INTEGER,
      NAME VARCHAR(255) CHARACTER SET LATIN NOT CASESPECIFIC,
      DATE_BIRTH DATE FORMAT 'YYYY-MM-DD'),
      AMOUNT FLOAT(10);