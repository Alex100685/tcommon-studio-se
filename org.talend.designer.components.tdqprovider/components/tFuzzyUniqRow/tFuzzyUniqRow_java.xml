
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="true"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
	<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR
      NAME="UNIQUE"
      CTYPE="FLOW"
      MAX_OUTPUT="1"
      COLOR="086438"
      BASE_SCHEMA="FLOW"
    />
    <CONNECTOR
      NAME="DUPLICATE"
      CTYPE="FLOW"
      MAX_OUTPUT="1"
      LINE_STYLE="2"
      COLOR="f36300"
      BASE_SCHEMA="FLOW"
    />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1"/>
    <PARAMETER NAME="SCHEMA_UNIQUE" FIELD="SCHEMA_TYPE" NUM_ROW="1" CONTEXT="UNIQUE">
      <TABLE READONLY="true">
        <COLUMN NAME="UID" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA_DUPLICATE" FIELD="SCHEMA_TYPE" NUM_ROW="1" CONTEXT="DUPLICATE">
      <TABLE READONLY="true">
        <COLUMN NAME="FID" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="UNIQUE_KEY"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="2"
      NB_LINES="5"
    >
      <ITEMS BASED_ON_SCHEMA="true">
      	<ITEM NAME="KEY_ATTRIBUTE" FIELD="CHECK" VALUE="false"/>
		<ITEM NAME="MATCHING_TYPE" FIELD="CLOSED_LIST">
			<ITEMS DEFAULT="EXACT_MATCH">
				<ITEM NAME="EXACT_MATCH" VALUE="EXACT_MATCH"/>
				<ITEM NAME="LEVENSHTEIN" VALUE="LEVENSHTEIN"/>
				<ITEM NAME="METAPHONE" VALUE="METAPHONE"/>
				<ITEM NAME="DOUBLE_METAPHONE" VALUE="DOUBLE_METAPHONE"/>
			</ITEMS>
		</ITEM>
		<ITEM NAME="CASE_SENSITIVE" FIELD="CHECK" VALUE="false" NOT_SHOW_IF="MATCHING_TYPE=='DOUBLE_METAPHONE'"/>
		<ITEM NAME="MIN_DISTANCE" FIELD="TEXT" VALUE="0"/>
		<ITEM NAME="MAX_DISTANCE" FIELD="TEXT" VALUE="0"/>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

  </ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
     <IMPORTS>
     	<IMPORT NAME="Commons-Codec" MODULE="commons-codec-1.3.jar" REQUIRED="true" />
        <IMPORT NAME="Commons-Lang" MODULE="commons-lang-2.1.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_UNIQUES" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_DUPLICATES" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>  
</COMPONENT>
