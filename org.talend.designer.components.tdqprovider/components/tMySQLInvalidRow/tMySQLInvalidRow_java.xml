<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070314A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:MYSQL"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
    
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tMysqlConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="4"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>MySQL</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	    <PARAMETER
		  NAME="ANALYZED_COLUMN"
		  FIELD="COLUMN_LIST"
		  REQUIRED="true"
		  NUM_ROW="90">
		</PARAMETER>
		<PARAMETER NAME="CUSTOM_PATTERN" FIELD="CHECK" NUM_ROW="100">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PATTERN_LIST" FIELD="CLOSED_LIST" NUM_ROW="100" SHOW_IF="CUSTOM_PATTERN == 'false'">
			<ITEMS>
			  <ITEM NAME="EMAIL_ADDRESS" VALUE="'[A-Z0-9._%-]+@[A-Z0-9.-]+\\\\.[A-Z]{2,4}'"/>
			  <ITEM NAME="EXTENDED_HOUR_TIME" VALUE="'^((([0]?[1-9]|1[0-2])(:|\\\\.)[0-5][0-9]((:|\\\\.)[0-5][0-9])?( )?(AM|am|aM|Am|PM|pm|pM|Pm))|(([0]?[0-9]|1[0-9]|2[0-3])(:|\\\\.)[0-5][0-9]((:|\\\\.)[0-5][0-9])?))$'"/>
			  <ITEM NAME="WEEK_DAY" VALUE="'^(Sun|Mon|(T(ues|hurs))|Fri)(day|\\\\.)?$|Wed(\\\\.|nesday)?$|Sat(\\\\.|urday)?$|T((ue?)|(hu?r?))\\\\.?$'"/>
			  <ITEM NAME="WEBSITE_URL" VALUE="'^https?://[[:alnum:]_-][\\\\.[[:alnum:]_-]*]*\\\\.(com|edu|org|net|int|info|eu|biz|mil|gov|aero|travel|pro|name|museum|coop|asia|[a-z][a-z])+(:[[:digit:]]+)?[/[[:alnum:]\\\\._#-\\\\?]*]*/?$'"/>
			  <ITEM NAME="INTEGER_VALUES" VALUE="'^[-+]?[1-9][[:digit:]]*\\\\.?[0]*$'"/>
			  <ITEM NAME="UK_PHONE_NUMBER" VALUE="'^(\\\\+44[[:space:]]?7[[:digit:]]{3}|\\\\(?07[[:digit:]]{3}\\\\)?)[[:space:]]?[[:digit:]]{3}[[:space:]]?[[:digit:]]{3}$'"/>
			  <ITEM NAME="BE_CODE_POSTAL" VALUE="'^(F-[0-9]{4,5}|B-[0-9]{4})$'"/>
			  <ITEM NAME="DE_POSTLEITZAHL_(POSTAL_CODE)" VALUE="'^[0-9]{5}$'"/>
			  <ITEM NAME="FR_CODE_POSTAL" VALUE="'^(0[1-9]|[1-9][0-9])[0-9]{3}$'"/>
			  <ITEM NAME="24_HOUR_TIME" VALUE="'^([0-1]?[0-9]|2[0-4]):([0-5][0-9])(:[0-5][0-9])?$'"/>
			  <ITEM NAME="ISO_DATE" VALUE="'^((((19|20)(([02468][048])|([13579][26]))-02-29))|((20[0-9][0-9])|(19[0-9][0-9]))-((((0[1-9])|(1[0-2]))-((0[1-9])|(1[[:digit:]])|(2[0-8])))|((((0[13578])|(1[02]))-31)|(((0[1,3-9])|(1[0-2]))-(29|30)))))$'"/>
			  <ITEM NAME="MONTH" VALUE="'^((0[1-9])|(1[0-2]))$'"/>
			  <ITEM NAME="AMERICAN_EXPRESS_CARD_NUMBER" VALUE="'^3[47][0-9]{13}$'"/>
			  <ITEM NAME="CONTAINS_NUMBERS" VALUE="'[[:digit:]]'"/>
			  <ITEM NAME="MASTER_CARD_NUMBER" VALUE="'^5[1-5][0-9]{14}$'"/>
			  <ITEM NAME="NUMERIC" VALUE="'^[[:digit:]]*$'"/>
			  <ITEM NAME="US_SSN" VALUE="'^([[:digit:]]{3}[ -][[:digit:]]{2}[ -][[:digit:]]{4}|[[:digit:]]{9})$'"/>
			  <ITEM NAME="BLANK_TEXT" VALUE="'^[[:blank:]]*$'"/>
			  <ITEM NAME="EMPTY_TEXT" VALUE="'^[[:space:]]*$'"/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="DQ_PATTERN" FIELD="TEXT" NUM_ROW="100" SHOW_IF="CUSTOM_PATTERN == 'true'">
			<DEFAULT>"''"</DEFAULT>
		</PARAMETER>

		
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
    	<IMPORT NAME="Driver-MySql" MODULE="mysql-connector-java-5.1.0-bin.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>