<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="dataprofiling">
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost/talend_dq</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.current_session_context_class">thread</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <mapping resource="org/talend/dataprofiler/persistence/TdqInterval.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqDayTime.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqValues.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqIndicatorValue.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqIndicatorDefinition.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqCalendar.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqAnalysis.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqAnalyzedElement.hbm.xml" />
        <mapping resource="org/talend/dataprofiler/persistence/TdqIndicatorOptions.hbm.xml" />
    </session-factory>
</hibernate-configuration>
