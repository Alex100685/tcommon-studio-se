<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.talend.core.repository_provider">
      <RepositoryFactory
            authenticationNeeded="false"
            class="org.talend.repository.localprovider.model.LocalRepositoryFactory"
            id="local"
            name="Local"/>
   </extension>
   <extension
         point="org.talend.core.global_actions">
      <GlobalAction class="org.talend.repository.localprovider.ui.actions.ReloadSystemRoutinesAction"/>
   </extension>
   <extension
         point="org.talend.core.migrationTask">
      <projecttask
            beforeLogon="true"
            class="org.talend.repository.localprovider.model.migration.RemoveRoutineFolderTask"
            id="org.talend.repository.localprovider.RemoveRoutineFolderTask"
            name="Remove routine folder"/>
   </extension>

</plugin>
