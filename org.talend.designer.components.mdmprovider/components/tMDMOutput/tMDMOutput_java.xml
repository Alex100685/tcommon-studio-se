<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20090320A"
		DATA_AUTO_PROPAGATE="false"
		STARTABLE="false" HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT />
		</PARAMETER>
		
	    <PARAMETER
	      NAME="SCHEMA_REJECT"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="11"
	      CONTEXT="REJECT"
	      SHOW="true">
	      <TABLE READONLY="true">
	        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	      </TABLE>
	    </PARAMETER> 
    
	    <PARAMETER
	      NAME="XMLFIELD"
	      FIELD="PREV_COLUMN_LIST"
	      REQUIRED="true"
	      NUM_ROW="15"
	    >
	    </PARAMETER>

		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"http://localhost:8080/talend/TalendPort"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>	
			
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
			NUM_ROW="30">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATAMODEL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ISUPDATE" FIELD="CHECK" REQUIRED="true"
				 NUM_ROW="45">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="WITHREPORT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SOURCE" FIELD="TEXT" NUM_ROW="60" REQUIRED="true"  SHOW_IF="WITHREPORT=='true'">
			<DEFAULT>"TOS"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ISINVOKE" FIELD="CHECK" REQUIRED="true" SHOW_IF="WITHREPORT=='true'"
				 NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="90">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>	
		<PARAMETER NAME="EXTENDINSERT" FIELD="CHECK" NUM_ROW="8"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMMIT_LEVEL" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="true" SHOW_IF="EXTENDINSERT == 'true'">
			<DEFAULT>200</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-Webservice" MODULE="mdm_webservice.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.5.1.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.0.4.jar" REQUIRED="true" /> 
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />  
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />   
    </IMPORTS>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>