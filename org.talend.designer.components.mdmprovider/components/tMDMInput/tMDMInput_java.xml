<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20090320A"
		STARTABLE="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

	   <PARAMETER
	      NAME="SCHEMA"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="10"
	    >
	    </PARAMETER>
	
	    <PARAMETER
	      NAME="XMLFIELD"
	      FIELD="COLUMN_LIST"
	      REQUIRED="true"
	      NUM_ROW="15"
	    >
	    </PARAMETER>

		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"http://localhost:8080/talend/TalendPort"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>	
			
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
			NUM_ROW="30">
			<DEFAULT></DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="CONCEPT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>		
    
	    <PARAMETER NAME="USE_ITEMS" FIELD="CHECK" REQUIRED="true" NUM_ROW="50">
	    	<DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	  	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="59" SHOW_IF="USE_ITEMS=='false'">
	    	<DEFAULT>Add all the key values below for the item</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER NAME="IDS" FIELD="TABLE" NUM_ROW="60" SHOW_IF="USE_ITEMS=='false'">
	    	<ITEMS>
	    		<ITEM NAME="IDVALUE" FIELD="TEXT" />
	    	</ITEMS>
	    </PARAMETER>
		
		<PARAMETER NAME="OPERATIONS" FIELD="TABLE" NUM_ROW="60" SHOW_IF="USE_ITEMS=='true'">
	      <ITEMS>
	        <ITEM NAME="XPATH" FIELD="TEXT" />
	        
	        <ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
	          <ITEMS DEFAULT="CONTAINS">	          	
	            <ITEM NAME="CONTAINS"  VALUE="CONTAINS"/>
	            <ITEM NAME="JOIN" VALUE="JOIN" />
	            <ITEM NAME="STARTSWITH"    VALUE="STARTSWITH"/>
	            <ITEM NAME="STRICTCONTAINS"    VALUE="STRICTCONTAINS"/>
	            <ITEM NAME="EQUAL"    VALUE="EQUALS"/>
	            <ITEM NAME="NOTEQUAL"    VALUE="NOT_EQUALS"/>
	            <ITEM NAME="GREATER"    VALUE="GREATER_THAN"/>
	            <ITEM NAME="GREATEROREQUAL"    VALUE="GREATER_THAN_OR_EQUAL"/>
	            <ITEM NAME="LOWER"    VALUE="LOWER_THAN"/>
	            <ITEM NAME="LOWEROREQUAL"    VALUE="LOWER_THAN_OR_EQUAL"/>
	          </ITEMS>
	        </ITEM>
	        
	        <ITEM NAME="VALUE" FIELD="TEXT" />
	        
	        <ITEM NAME="PREDICATE" FIELD="CLOSED_LIST">
	        	<ITEMS DEFAULT="NONE">
	        		<ITEM NAME="NONE" VALUE="NONE" />
	        		<ITEM NAME="OR" VALUE="OR" />
	        		<ITEM NAME="AND" VALUE="AND" />	        		
	        		<ITEM NAME="STRICTAND" VALUE="STRICTAND" />
	        		<ITEM NAME="EXACTLY" VALUE="EXACTLY" />
	        		<ITEM NAME="NOT" VALUE="NOT" />
	        	</ITEMS>
	        </ITEM>
	        
	      </ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="SPELLTHREHOLD" FIELD="TEXT" NUM_ROW="70" SHOW_IF="USE_ITEMS=='true'">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SKIP" FIELD="TEXT" NUM_ROW="70" SHOW_IF="USE_ITEMS=='true'" >
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="MAXVALUE" FIELD="TEXT" NUM_ROW="70" SHOW_IF="USE_ITEMS=='true'" >
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		
		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>	
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-Webservice" MODULE="mdm_webservice.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.5.1.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" /> 
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />  
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />   
    </IMPORTS>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>